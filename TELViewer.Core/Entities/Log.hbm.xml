<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TELViewer.Core" namespace="TELViewer.Core.Entities" >
  <class name="Log" table="Log" lazy="true">
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Date" />
    <property name="Thread"  />
    <property name="Level" />
    <property name="Logger" />
    <property name="Message" />
    <property name="Exception" />
  </class>
</hibernate-mapping>